{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true,
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "browser": true,
    "es6": true,
  },
  "globals": {
  },
  "rules": {
    "semi": [
      "error",
      "never"
    ]
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "eslint:recommended"
  ]
}