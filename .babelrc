{
	"env": {
		"production": {
			"plugins": [
				["react-remove-properties", {"properties": ["data-test"]}]
			]
		}
	},
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": "> 0.25%, not dead",
				"useBuiltIns": "usage",
				"corejs": "3.6.5"
			}
		],
		"@babel/preset-react"
	],
	"plugins": [
		"@babel/plugin-syntax-dynamic-import",
		"@babel/plugin-proposal-object-rest-spread",
		"@babel/plugin-proposal-class-properties"
	]
}
