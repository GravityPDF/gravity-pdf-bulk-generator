{
	"env": {
		"production": {
			"plugins": [
				["react-remove-properties", {"properties": ["data-test"]}]
			]
		}
	},
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"browsers": [
						"last 2 versions"
					]
				},
				"forceAllTransforms": true,
				"useBuiltIns": "usage",
				"corejs": "3.6"
			}
		],
		"@babel/preset-react"
	],
	"plugins": [
		"@babel/plugin-syntax-dynamic-import",
		"@babel/plugin-proposal-object-rest-spread",
		"@babel/plugin-proposal-class-properties"
	]
}
